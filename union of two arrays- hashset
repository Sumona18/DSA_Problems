class Solution {
  public:
    vector<int> findUnion(vector<int>& a, vector<int>& b) {
        // code here
       unordered_set<int> c;
       
       a.insert(a.end(), b.begin(), b.end());
      for(int i = 0; i< a.size(); i++){
          c.insert(a[i]);
      }
       a.assign(c.begin(), c.end());
       return a ;
    }
};